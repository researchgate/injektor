{
  "name": "rg/injektor",
  "description": "Dependency Injection container inspired by google-guice",
  "keywords": [
    "dependency",
    "injection"
  ],
  "homepage": "http://github.com/researchgate/injektor",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Bastian Hofmann",
      "email": "bastian.hofmann@researchgate.net",
      "homepage": "http://www.bastianhofmann.de"
    }
  ],
  "require": {
    "php": ">=8.0.",
    "doctrine/annotations": "^2.0.0",
    "laminas/laminas-code": "^4.7.0",
    "psr/log": "^1.0.0"
  },
  "require-dev": {
    "friendsofphp/proxy-manager-lts": "^1.0",
    "phpunit/phpunit": "^9.0.0",
    "roave/better-reflection": "^4.0.0 || ^5.0.0"
  },
  "suggest": {
    "ocramius/proxy-manager": "For lazy loading"
  },
  "autoload": {
    "psr-0": {
      "rg\\injektor": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
